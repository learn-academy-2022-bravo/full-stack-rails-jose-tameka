<h3>Add a New Blog Entry</h3>

<%= form_with model: @bravo_blog do |form| %>

    <%= form.label :title %>
    <%= form.text_field :title %>

    <br />
    <%= form.label :content %>
    <%= form.text_field :content %>

    <br />
    <%= form.submit 'Add Entry' %>

<% end %>

<%= link_to 'HOME', entries_path %>